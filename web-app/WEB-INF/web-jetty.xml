<Configure class="org.mortbay.jetty.webapp.WebAppContext">
  	<Get name="securityHandler">
	    <Set name="userRealm">
	      	 <New class="org.mortbay.jetty.plus.jaas.JAASUserRealm">
			      <Set name="name">gdocRealm</Set>
			      <Set name="loginModuleName">gdoc</Set>
			  </New>
	    </Set>
	  </Get>                                                          
</Configure>